{
    "name": "PayPal Form Filler",
    "version": "1.0",
    "description": "Testing out guest checkout on PayPal? With one click! This extension will fill out the PayPal guest checkout form!",
    "permissions": ["tabs", "activeTab", "declarativeContent","https://www.fakenamegenerator.com/gen-random-us-us.php"],
    "web_accessible_resources" : ["fillForm.js"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "page_action": {
        "default_title": "PayPal Form Filler",
        "default_icon": {
          "16": "images/FF16.png",
          "32": "images/FF32.png",
          "48": "images/FF48.png",
          "128": "images/FF128.png"
        }
      },
      "icons": {
        "16": "images/FF16.png",
        "32": "images/FF32.png",
        "48": "images/FF48.png",
        "128": "images/FF128.png"
      },
    "manifest_version": 2
}